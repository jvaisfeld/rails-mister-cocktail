<div id="bar-banner" class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('data:image/jpeg;base64,');">
  <div class="banner-content">
    <h1>COCKTAILS</h1>
  </div>
</div>

<div class="container">
  <div class="row">

  <% @cocktails.each do |c| %>
    <div class="col-xs-12 col-md-4 col-med-offset-3">
      <% if c.photo? %>
        <%= render "cocktail", cocktail: c %>
      <% else %>
        <%= render "cocktail_no_photo", cocktail: c %>
      <% end %>
    </div>
  <% end %>
  </div>
</div>



